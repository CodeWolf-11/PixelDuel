    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/moment": "^2.11.29",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.15",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.4",
    "prisma": "^5.19.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
