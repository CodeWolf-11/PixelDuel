{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/index.js",
    "server": "nodemon ./dist/index.js",
    "watch": "tsc -w",
    "build": "concurrently \" tsc --build \" \" npm run copy-file\"",
    "dev": "concurrently \"npm run watch \" \"npm run server\" \"npm run watch-views\"",
    "copy-file": "copyfiles -u 1 \"src/views/**/*\"  dist/",
    "watch-views": "nodemon --watch src/views -e ejs -x \"npm run copy-file\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
